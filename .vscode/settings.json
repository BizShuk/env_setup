{
    "mcp": {
        "excluded": [
            "context7",
            "senera",
            "github",
            "dbhub"
        ]
    },
    "mcpServers": {},
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "{.svn,.hg,.DS_Store,Thumbs.db}": true,
        "archive": true,
        "**/homebrew": true,
        "{.git,.gitlab,.pre-commit-config.yaml}": true,
        "{*.code-workspace,.golangci.yml,go.sum}": true,
        "{.agent,.claude,.gemini,.serena,.ttadk}": true,
        "{AGENTS.md,GEMINI.md,CLAUDE.md}": true,
        "{.vscode}": false,
        "{.devops,.settings}": true,
        "{.classpath,.project}": true,
        "{target,out,dist,output}": true,
        "{.mvn,node_modules,__pycache__,.venv}": true,
        "{__debug_bin*}": true,
        "{gen/**,kitex_gen}": true,
        "-1": true,
        "{.agent,**/.gemini,.serena}": false,
        "AI/.agent/history": true,
        "AI/.agent/tmp": true,
        "AI/.agent/google_accounts.json": true,
        "AI/.agent/installation_id": true,
        "AI/.agent/oauth_creds.json": true,
        "AI/.agent/settings.json.orig": true,
        "AI/.agent/state.json": true,
        "AI/.agent/trusted_hooks.json": true,
        "AI/.agent/antigravity-browser-profile": true,
        "AI/.agent/antigravity": true
    },
    "explorerExclude.backup": {},
    "hooks": {}
}