{
    "ide": {
        "enabled": true,
        "hasSeenNudge": true
    },
    "security": {
        "auth": {
            "selectedType": "oauth-personal"
        },
        "disableYoloMode": true,
        "confirmCommands": "sensitive-only"
    },
    "ui": {
        "accessibility": {
            "screenReader": false
        },
        "showStatusInTitle": false,
        "showMemoryUsage": true
    },
    "general": {
        "previewFeatures": true,
        "enablePromptCompletion": true
    },
    "tools.allowed": [
        "echo",
        "afplay"
    ],
    "hooks": {},
    "experimental": {
        "skills": true
    },
    "skills": {
        "disabled": [
            "code-reviewer"
        ]
    },
    "mcpServers": {
        "senera": {
            "command": "uvx",
            "args": [
                "--from",
                "git+https://github.com/oraios/serena",
                "serena",
                "start-mcp-server"
            ]
        },
        "docker-mcp": {
            "command": "uvx",
            "args": [
                "docker-mcp"
            ]
        },
        "gopls-mcp": {
            "command": "gopls",
            "args": [
                "mcp"
            ]
        }
    }
}