{
    "model": {
        "name": "gemini-3-flash-preview"
    },
    "ide": {
        "enabled": true,
        "hasSeenNudge": true
    },
    "security": {
        "auth": {
            "selectedType": "oauth-personal"
        },
        "disableYoloMode": true,
        "confirmCommands": "sensitive-only"
    },
    "ui": {
        "accessibility": {
            "screenReader": false
        },
        "showStatusInTitle": false,
        "showMemoryUsage": true
    },
    "general": {
        "vimMode": false,
        "previewFeatures": true,
        "enablePromptCompletion": true,
        "preferredEditor": "vscode",
        "checkpointing": {
            "enabled": true
        }
    },
    "tools": {
        "shell": {
            "showColors": true
        },
        "autoAccept": true,
        "approvalMode": "auto_edit",
        "allowed": [
            "echo",
            "afplay"
        ]
    },
    "hooksConfig": {
        "enabled": true
    },
    "hooks": {},
    "experimental": {
        "skills": true,
        "contextCaching": true,
        "cacheTtlSeconds": 3600
    },
    "skills": {
        "disabled": [
            "code-reviewer"
        ]
    },
    "mcpServers": {
        "docker": {
            "command": "uvx",
            "args": [
                "docker-mcp"
            ],
            "trust": false
        },
        "gopls": {
            "command": "gopls",
            "args": [
                "mcp"
            ],
            "trust": false
        },
        "dbhub": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "@bytebase/dbhub@latest",
                "--transport",
                "stdio",
                "--config=./config/db.toml"
            ],
            "trust": true
        }
    }
}