#!/bin/bash
# you need to get kubectl first that build by kubernets cluster

./kubectl config view | grep current-context | awk '{print $2}'
echo 'rc---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------'
./kubectl get rc
echo 'po---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------'
./kubectl get po
echo 'se---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------'
./kubectl get se
echo 'secret-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------'
./kubectl get secret
echo 'nodes------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------'
./kubectl get nodes
echo 'serviceaccount---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------'
./kubectl get serviceaccount
echo 'namespace--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------'
./kubectl get namespace
echo 'events-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------'
./kubectl get events | tail -n 5
