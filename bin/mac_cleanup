#!/bin/bash


# Clear system logs and temporary files:
sudo rm -rf /private/var/log/*
sudo rm -rf /private/var/tmp/*
sudo rm -rf /Library/Logs/*

# Clear user cache:
rm -rf ~/Library/Caches/*
# Clear system cache:
sudo rm -rf /Library/Caches/*

# Remove old Time Machine backups:
sudo tmutil deletelocalsnapshots /

# Remove unused Docker images and containers:
# check whether cmd docker exists if so run the command
command -v docker >/dev/null 2>&1 && docker system prune -a

# Empty the Trash:
rm -rf ~/.Trash/*


# Lark
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/log/*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/1*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/2*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/3*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/4*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/5*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/6*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/7*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/8*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/9*
rm -rf /Users/shuk/Library/Application Support/LarkInternational/sdk_storage/0*

# Cache
rm -rf /Users/shuk/Library/Caches/Google/Chrome/Default/Cache/Cache_Data/*
rm -rf /Users/shuk/Library/Caches/*

# Golang
rm -rf /Users/shuk/projects/go_project/src/*

# iMovie/iTunes
rm -rf /Users/shuk/Music/*

# Whatapps
rm -rf /Users/shuk/Library/Group\ Containers/group.net.whatsapp.WhatsApp.shared/Message/Media/*

# WeChat
rm -rf /Users/shuk/Library/Containers/com.tencent.xinWeChat/Data
rm -rf /Users/shuk/Library/Containers/com.tencent.xinWeChat.WeChatMacShare/Data

brew bundle cleanup